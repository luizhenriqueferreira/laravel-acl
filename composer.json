{
    "name": "luizhenriqueferreira/laravel-acl",
    "description": "ACL package for Laravel",
    "type": "package",
    "keywords": [
        "Luiz Henrique ferreira",
        "laravel",
        "acl",
        "auth",
        "role",
        "permission",
        "laravel 5",
        "laravel 6",
        "laravel 7",
        "laravel 8",
        "security"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Luiz Henrique Ferreira",
            "email": "luizhenriquesf@objetus.com.br",
            "homepage": "http://luizhenriquedigital.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/container": "^6.20|^7.0|^8.0",
        "illuminate/contracts": "^6.20|^7.0|^8.0",
        "illuminate/database": "^6.20|^7.0|^8.0",
        "illuminate/support": "^6.20|^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LuizHenriqueFerreira\\LaravelAcl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LuizHenriqueFerreira\\LaravelAcl\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LuizHenriqueFerreira\\LaravelAcl\\LaravelAclServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests/",
        "check-style": "php-cs-fixer fix --diff --verbose --dry-run .",
        "fix-style": "php-cs-fixer fix ."
    },
    "config": {
        "sort-packages": true
    }
}
